        function doFilter() {
            filter = document.getElementById("the-filter").value.toLowerCase();
            doIt();
        }
        function doClear() {
            el = document.getElementById("the-filter");
            if (filter) {
                filter = '';
                el.value = filter;
                doIt();
            }
            el.focus();
        }
        function append(newChild, id) {
            if (id == null) {
                document.body.appendChild(newChild);
            }
            else {
                const node = document.getElementById(id);
                node.appendChild(newChild);
            }
        }
        function gen_cell(tr, type) {
            const td1 = document.createElement(type);
            td1.setAttribute("align", "left");
            style = "padding: 5px 10px 5px 5px;"
            tr.appendChild(td1);
            return td1;
        }
        function gen_th() {
            const tr = document.createElement("tr");
            gen_cell(tr, "th").appendChild(gen_p("Name"))
            gen_cell(tr, "th").appendChild(gen_p("User"))
            gen_cell(tr, "th").appendChild(gen_p("User"))
            gen_cell(tr, "th").appendChild(gen_p("Pw"))
            gen_cell(tr, "th").appendChild(gen_p("Site"))
            gen_cell(tr, "th").appendChild(gen_p("Notes"))
            return tr;
        }
        function gen_tr(forSite, thePw, user, theLink, notes) {
            const tr = document.createElement("tr");
            if (!notes)
                notes = '';
            gen_cell(tr, "td").appendChild(gen_p(forSite));
            gen_cell(tr, "td").appendChild(gen_p(user));
            gen_cell(tr, "td").appendChild(gen_b1(user));
            gen_cell(tr, "td").appendChild(gen_b(thePw));
            gen_cell(tr, "td").appendChild(gen_a(theLink))
            gen_cell(tr, "td").appendChild(gen_p(notes))
            let td = gen_cell(tr, "td");
            td.appendChild(gen_p(thePw));
            td.style.display = "none";
            return tr;
        }
        function gen_p(text) {
            return document.createTextNode(text);
        }
        function gen_hidden(text, idNum) {
            let n = document.createElement("textarea");
            n.id = "pw-" + (idNum).toString(10);
            n.style.display = "none";
            n.innerText = text;
            return n;
        }
        function gen_i(val) {
            const el = document.createElement("input");
            el.value = val;
            // el.setAttribute("value", val);
            el.setAttribute("size", "35");
            return el;
        }
        function gen_a(val) {
            var el = document.createElement("a");
            el.href = val;
            el.target = "_blank";
            el.innerHTML = val;
            return el;
        }
        var id_num = 0;
        function gen_b(val) {
            var h = gen_hidden(val, ++id_num);
            var el = document.createElement("button");
            var id = "btn-" + (++id_num).toString(10);
            el.innerText = " pw ";
            el.id = id;
            el.setAttribute("onClick", "setClip('" + h.id + "')");
            el.appendChild(h);
            return el;
        }
        function gen_b1(val) {
